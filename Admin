//

public class Admin {
	private String name;
	private int ID;
	
	//constructor
	public Admin() {
		name = "admin";
		ID = 00000;
	}
	
	//get/set
	public String getName() {
		return name;
	}
	
	public int getID () {
		return ID;
	}
	
	public void setName(String n) {
		name = n;
	}
		
	public void setID(int i) {
		ID = i;
	}
		
	//toString
	public String toString() {
		return "Name: " + name + "\nID: " + ID;
	}

}
