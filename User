//

public class User {
	private String name;
	private int ID;
	
	//constructor
	public User (String name, int i) {
		name = n;
		ID = i;
	}
	
	//get/set
	public String getName() {
		return name;
	}
	
	public int getID() {
		return ID;
	}
	
	public void setName(String n) {
		name=n;
	}
	
	public void setID(int i) {
		ID = i;
	}
	
	//toString
	public String toString() {
		return "Name: " + name + "\nID: " + ID;
	}
}
